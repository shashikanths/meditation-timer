{
  "name": "instant-om",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vitest run && vite build",
    "build:skip-tests": "vite build",
    "preview": "vite preview",
    "server": "node server/index.cjs",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "seed": "node server/seed.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "deploy": "npm run build && firebase deploy",
    "deploy:hosting": "npm run build && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:firestore": "firebase deploy --only firestore"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "firebase": "^12.7.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "better-sqlite3": "^12.5.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "happy-dom": "^20.3.1",
    "multer": "^2.0.2",
    "typescript": "~5.8.2",
    "uuid": "^13.0.0",
    "vite": "^6.2.0",
    "vitest": "^4.0.17"
  }
}
